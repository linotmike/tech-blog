<main id="home-card">

 

 

    {{#each topics as |topic|}}
    <div id="profile-card" class="card" style="width: 18rem;">
      <div class="card-body">
        <h3 id="post-home"><a href="/post/{{topic.id}}" data-replace="get a link">{{topic.title}}</a></h3>
        <p>By: {{user.username}}</p>
        <p>created at : {{format_date topic.createdAt}}</p>
      </div>
    </div>
    {{/each}}
</main>